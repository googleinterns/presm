{
  "name": "presm",
  "version": "0.0.0",
  "description": "Pre-processing for ES modules in node",
  "devDependencies": {
    "@types/node": "^14.0.12",
    "@types/tape": "^4.13.0",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "gts": "^2.0.2",
    "prettier": "^2.0.5",
    "tape": "^5.0.1",
    "typescript": "^3.9.5",
    "yaml": "^1.10.0",
    "yargs": "^15.3.1"
  },
  "engines": {
    "node": ">=12.2.0"
  },
  "scripts": {
